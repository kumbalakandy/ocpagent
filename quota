apiVersion: v1
kind: ResourceQuota
metadata:
  name: test-project-gpu-quota
  namespace: test-project
spec:
  hard:
    requests.nvidia.com/l40s: "2"
    requests.nvidia.com/h200: "10"
	

apiVersion: v1
kind: Pod
metadata:
  name: gpu-pod
spec:
  containers:
  - name: gpu-container
    image: <your-image>
    resources:
      requests:
        nvidia.com/l40s: 1
        nvidia.com/h200: 1
